<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mrz.dao.CyDao">
    <select id="getCyByPyFirst" resultType="Cy">
        select *
        from cy
        where py_first=#{pyFirst}
    </select>
    <select id="getCyByPynd" resultType="Cy">
        select *
        from cy
      where py_end = #{pyEnd}
    </select>
    <select id="getCy" resultType="Cy">
        select *
        from cy
        where cy=#{cy}
    </select>
</mapper>